                    "content": [
                        {
                            "subTitle": "Android",
                            "linkTitle": "",
                            "Link": "",
                            "img_url": "",
                            "text": "Sonolus is currently under development, thus it is not yet available in Android stores.\nTo install Sonolus on Android device, use any of the methods below.",
                            "isNative": false
                        },
                        {
                            "subTitle": "Sideload",
